# Environment variables for Monad testnet deployment
# Copy this file to .env and fill in your values

# Private key for deployment (without 0x prefix)
PK=your_private_key_here

# Monad testnet RPC URL
RPC_URL=https://testnet-rpc.monad.xyz

# ConditionalTokens Framework address on Monad testnet
CTF_ADDRESS=0x52CD9f25AFcc9D007F72431842F811E8F21EE75F

# Admin address for the TrustedCtfAdapter (optional)
# If not set, the deployer will be the admin
# ADMIN=0x1234567890123456789012345678901234567890
